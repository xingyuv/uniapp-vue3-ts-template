    "@dcloudio/uni-mp-lark": "3.0.0-alpha-3070020230114001",
    "@dcloudio/uni-mp-qq": "3.0.0-alpha-3070020230114001",
    "@dcloudio/uni-mp-toutiao": "3.0.0-alpha-3070020230114001",
    "@dcloudio/uni-mp-weixin": "3.0.0-alpha-3070020230114001",
